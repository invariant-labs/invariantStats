{
  "version": 2,
  "github": {
    "enabled": false
  },
  "rewrites": [
    {
      "source": "/((?!a0)(?!svm).*)",
      "destination": "/api/svm/$1"
    },
    {
      "source": "/a0/(.*)",
      "destination": "/api/a0/$1"
    },
    {
      "source": "/svm/(.*)",
      "destination": "/api/svm/$1"
    }
  ]
}
