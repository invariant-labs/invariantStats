{
  "version": 2,
  "github": {
    "enabled": false
  },
  "rewrites": [
    {
      "source": "/((?!vara)(?!alph)(?!a0)(?!svm)(?!solana).*)",
      "destination": "/api/svm/$1"
    },
    {
      "source": "/a0/(.*)",
      "destination": "/api/a0/$1"
    },
    {
      "source": "/alph/(.*)",
      "destination": "/api/alph/$1"
    },
    {
      "source": "/vara/(.*)",
      "destination": "/api/vara/$1"
    },
    {
      "source": "/svm/(.*)",
      "destination": "/api/svm/$1"
    },
    {
      "source": "/solana/(.*)",
      "destination": "/api/solana/$1"
    }
  ]
}
